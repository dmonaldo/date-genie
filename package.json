  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Dante Monaldo <mail@dantemonaldo.com> (https://dantemonaldo.com)",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.3"
  }
}
